{
  "name": "nest-view",
  "version": "1.0.0",
  "description": "Module created to make ease data manipulation in Nestjs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "default": "./dist/src/index.js"
    },
    "./decorators/view": {
      "types": "./dist/src/decorators/swagger.d.ts",
      "default": "./dist/src/decorators/swagger.js"
    },
    "./decorators/swagger": {
      "types": "./dist/src/decorators/view.d.ts",
      "default": "./dist/src/decorators/view.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "npm run build",
    "prebuild": "rimraf dist",
    "build": "ttsc -p ./tsconfig.build.json && tsc-alias",
    "test": "jest",
    "test:app": "rimraf dist && nest start --watch"
  },
  "author": "j0hnsons",
  "license": "ISC",
  "dependencies": {
    "@nestjs/common": "^8.2.6",
    "@nestjs/swagger": "^5.2.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.0",
    "@nestjs/core": "^8.2.6",
    "@nestjs/platform-express": "^8.3.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.18",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "jest": "^27.4.7",
    "rxjs": "^7.5.2",
    "swagger-ui-express": "^4.3.0",
    "ts-jest": "^27.1.3",
    "tsc-alias": "^1.6.0",
    "ttypescript": "^1.5.13",
    "typescript": "^4.5.5",
    "typescript-transform-paths": "^3.3.1"
  }
}
