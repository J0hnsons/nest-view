{
  "name": "nest-view",
  "version": "1.0.7",
  "description": "Module created to make ease data manipulation in Nestjs",
  "author": "j0hnsons",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/J0hnsons/nest-view"
  },
  "bugs": {
    "url": "https://github.com/J0hnsons/nest-view/issues"
  },
  "homepage": "https://github.com/J0hnsons/nest-view#readme",
  "scripts": {
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "prebuild": "rimraf src",
    "build": "tsc -p ./tsconfig.build.json",
    "test": "jest",
    "test:app": "rimraf src && nest start --watch",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.build.json"
  },
  "dependencies": {
    "@nestjs/common": "^8.2.6",
    "@nestjs/swagger": "^5.2.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.0",
    "@nestjs/core": "^8.2.6",
    "@nestjs/platform-express": "^8.3.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.18",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "rxjs": "^7.5.2",
    "swagger-ui-express": "^4.3.0",
    "ts-jest": "^27.1.3",
    "tsc-alias": "^1.6.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "ttypescript": "^1.5.13",
    "typescript": "^4.5.5",
    "typescript-transform-paths": "^3.3.1"
  },
  "peerDependencies": {
    "@nestjs/common": "^8.2.6",
    "@nestjs/swagger": "^5.2.0",
    "reflect-metadata": "^0.1.13"
  },
  "keywords": [
    "nestjs",
    "nest",
    "view"
  ]
}
